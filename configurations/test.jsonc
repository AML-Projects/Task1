{
    "environment": {
        "name" : "describe the run",
        "output_path": "trainings",
        "log_file" : "logs.txt"
    },
    "imputer": {
        "name": "median"
    },
    "outliers": {
		"name": "lof",
		"fit_on" : "train",
		"customOR": {
			"method" : "iqr",
			"threshold" : 10
		}
    },
    "feature_selector": {
		"remove_features_with_many_Nan" : "False",
        "remove_constant_features" : "True",
		"remove_constant_features_par": {
			"threshold": 0.01
		},
        "remove_correlated_features" : "True",
		"remove_correlated_features_par" : {
			"threshold" : 0.8
		},
        "selectBestK" : "True",
		"selectBestK_par" : {
			"k" : 200
		},
        "selectBestBasedOnImpurity" : "False",
    },
	"normalizer": {
		"name": "robustscaler",
		"fit_on" : "train"
	},
	"regression": {
		"name": "xgb"
	},
	"search": {
	    "imputer": {
            "iterative_n_nearest_features" : [10],
	        "knn_n_neighbors" : [20],
            "knn_weights": ["uniform", "distance"],
            "name": ["mean", "median"],
	    },
	    "outlier": {
            "fit_on": ["train", "test", "both"],
	        "name": ["customOR", "lof", "iforest"],
	        "strategy":["iqr"],
	        "threshold": [10],
	    },
	    "feature_selector": {
	        "k" : [100, 200, 300],
	        "remove_constant": ["True"],
	        "remove_constant_threshold": [0.01],
            "remove_correlated":["True"],
            "remove_correlated_threshold":[0.8],
            "use_select_best_based_on_impurity" : ["False"],
            "use_select_best_k" : ["True"],
	    },
        "normalizer": {
            "fit_on": ["train", "test", "both"],
            "name" : ["stdscaler", "robustscaler"]
        },
        "regression":{
            "name":["xgb"]
        }
	}
}