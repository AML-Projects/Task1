{
    "environment": {
        "name" : "describe the run",
        "output_path": "trainings",
        "log_file" : "logs.txt"
    },
    "imputer": {
        "name": "median"
    },
    "outliers": {
		"name": "lof",
		"fit_on" : "train",
		"customOR": {
			"method" : "iqr",
			"threshold" : 10
		}
    },
    "feature_selector": {
		"remove_features_with_many_Nan" : "False",
        "remove_constant_features" : "True",
		"remove_constant_features_par": {
			"threshold": 0.01
		},
        "remove_correlated_features" : "True",
		"remove_correlated_features_par" : {
			"threshold" : 0.8
		},
        "selectBestK" : "True",
		"selectBestK_par" : {
			"k" : 200
		},
        "selectBestBasedOnImpurity" : "False",
    },
	"normalizer": {
		"name": "robustscaler",
		"fit_on" : "train"
	},
	"regression": {
		"name": "xgb"
	},
	"search": {
	    "imputer": {
	        "name": ["mean", "median"]
	    },
	    "outlier": {
            "fit_on": ["train", "test", "both"],
	        "name": ["customOR", "lof", "iforest"],
	        "strategy":["iqr"],
	        "threshold": [10],
	    },
	    "feature_selector": {
	        "corr_threshold" : [0.8, 0.9],
	        "k" : [100, 200, 300]
	    },
        "normalizer": {
            "name" : ["stdscaler", "minmaxscaler", "robustscaler"]
        },
        "regression":{
            "name":["xgb"]
        }
	}
}